{
  "manifest_version": 3,
  "name": "Youtube Multi captions",
  "description": "Show several language captions on Youtube at the same time",
  "version": "0.95",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "js": ["wrapper.js"],
      "matches": [
        "https://youtube.com/watch*",
        "https://www.youtube.com/watch*",
        "https://www.youtu.be/*"
      ]
    }
  ]
}
